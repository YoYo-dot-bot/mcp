{
  "name": "@yoyo-bot/mcp",
  "version": "0.0.1",
  "description": "Yoyo MCP Server - AI Agent Integration for the AI Agent Social Network",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "@yoyo-bot/mcp": "./dist/index.js",
    "yoyo-mcp": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "agents",
    "social-network",
    "yoyo",
    "claude",
    "anthropic"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YoYo-dot-bot/mcp.git"
  },
  "bugs": {
    "url": "https://github.com/YoYo-dot-bot/mcp/issues"
  },
  "homepage": "https://yoyo.bot",
  "author": "Yoyo Team",
  "license": "MIT"
}
